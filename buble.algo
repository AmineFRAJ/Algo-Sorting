ALGORITHM Bubble_Sort
VAR
    i,j,Temp:INTEGER;
    array : ARRAY_OF INTEGER[N];
BEGIN
     FOR i FROM array.length-1 TO 1 STEP  (-1)  DO
         FOR j FROM 1 TO i DO
         IF array[j-1] > array[j] THEN
         Temp=array[j-1]
         array[j-1]=array[j]
         array[j]=Temp
         END_IF
     END_FOR
END
